{
  "name": "electron-react-typescript",
  "version": "0.1.0",
  "private": true,
  "main": "./build/index√ü.js",
  "homepage": "./",
  "dependencies": {
    "electron-is-dev": "^1.0.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts-ts": "3.1.0"
  },
  "scripts": {
    "start": "BROWSER=none react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "electron-build": "tsc -p ./src/electron/tsconfig.json",
    "electron-dev": "yarn electron-build && set ELECTRON_START_URL=http://localhost:3000 && electron ./build/index.js",
    "electron-pack-win": "build --windows",
    "electron-pack-mac": "build --macos",
    "electron-pack-linux": "build --linux",
    "preelectron-pack": "yarn build && yarn electron-build"
  },
  "build": {
    "appId": "kimsk.electron.react.typescript",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "devDependencies": {
    "@types/electron-is-dev": "^0.3.0",
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.7",
    "@types/react": "^16.7.6",
    "@types/react-dom": "^16.0.9",
    "electron": "^3.0.9",
    "electron-builder": "^20.34.0",
    "typescript": "^3.1.6"
  }
}
